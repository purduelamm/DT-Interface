<mujoco model="core_maker">
    <compiler angle="radian" meshdir="meshes" autolimits="true"/>

    <option integrator="implicitfast"/>

    <default>
        <default class="visual">
        <geom group="2" type="mesh" contype="0" conaffinity="0" density="0"/>
        </default>
        <default class="collision">
        <geom group="3" type="mesh" density="2700"/>
        </default>
    </default>

    <asset>
        <mesh name="machine_body" file="core_maker.stl" scale="0.001 0.001 0.001"/>
        <mesh name="collision_body" file="core_maker.stl" scale="0.001 0.001 0.001"/>
    </asset>
    <worldbody>
        <body name="body">
            <geom mesh="machine_body" class="visual"/>
            <!-- <geom mesh="collision_body" class="collision"/> -->
        </body>
    </worldbody>
</mujoco>